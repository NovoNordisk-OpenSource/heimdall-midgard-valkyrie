{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Heimdall.Midgard.Valkyrie.Host.Api": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "discovery/v1/openapi/schema.json",
      "applicationUrl": "https://localhost:54324;http://localhost:54323",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/discovery/v1/openapi/schema.json",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}
