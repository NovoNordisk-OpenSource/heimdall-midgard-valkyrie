{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "ClientId": "a89ab03a-8746-4685-b05e-64a675ae6e94",
    "TenantId": "fdfed7bd-9f6a-44a1-b694-6e39c468c150"
  },
  "ConnectionStrings": {
    "ApplicationContext": "User ID=postgres;Password=local;Host=localhost;Port=5432;Database=postgres"
  },
  "GraphBeta": {
    "BaseUrl": "https://graph.microsoft.com/beta",
    "Scopes": [
      "user.read"
    ]
  },  
  "Kafka": {
    "Topics": [ "topic" ],
    "Configuration": {
      "bootstrap.servers": "kafka:9092",
      "group.id": "heimdall-consumer-group",
      "enable.auto.commit": false,
      "session.timeout.ms": 6000,
      "auto.offset.reset": "earliest",
      "security.protocol": "SASL_SSL",
      "sasl.mechanism": "PLAIN"
    },
    "EnablePartitionEof": true,
    "StatisticsIntervalMs": 5000
  },
  "KeyVaultName": "[Enter_the_KeyVault_Name_Here]",
  "OpenTelemetryExporter": {
    "Endpoint": "https://heimdall.dfplatformtool-prd.pub.azure.novonordisk.com/otlp/http"
  }
}
